<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <h1 class="page-header">SongList</h1>

    <% if notice.present? %>
      <div class="alert alert-info" role="alert">
        <%= notice %>
      </div>
    <% end %>

    <%= render "form", song: @song %>

    <hr />

    <table class="table table-striped" id="songList">
      <%= render @songs %>
    </table>

    <div class="footer">
      <a href="" id="Delete", onclick="deleteSong();">Delete</a>
      <a href="" id="Delete-all">Delete-all</a>
    </div>
  </div>
</div>

<p><%=link_to "[back to]", artist_path(@artist.id)%></p>
