<%= form_for @song do |f| %>
  <div class="form-group">
  <label>Song Title</label>
    <%= f.text_field :title, placeholder: "title", class: "form-control" %>
  </div>
  <div class="form-group">
    <label>Enter Song-link</label>
    <%= f.text_field :song_url, placeholder: "song-link(http://)", class: "form-control" %>
  </div>
  <div class="actions">
    <%= f.submit "Save", class: "btn btn-primary" %>
  </div>
<% end %>
